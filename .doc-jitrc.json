{
  "patterns": {
    "**/*.component.tsx": {
      "label": "Creating a React Component",
      "uri": "https://link-to-component-doc.com"
    },
    "**/*.hook.ts": "https://link-to-hook-doc.com",
    "**/user/*.ts": [
      "https://link-to-user-ts-files.com",
      "https://link-to-ts-files.com"
    ],
    "**/**.spec.ts, **/**.test.ts": {
      "label": "Vitest documentation",
      "uri": "https://vitest.dev/"
    },
    "package.json": {
      "label": "NPM Js website",
      "uri": "http://npmjs.com"
    },
    "**/*.test.ts, **/*.test.tsx": [
      {
        "label": "Jest documentation",
        "uri": "https://jestjs.io/docs/getting-started"
      }
    ],
    "**/*.test.tsx": [
      {
        "label": "React Native testing library documentation",
        "uri": "https://callstack.github.io/react-native-testing-library/docs/getting-started"
      }
    ]
  }
}